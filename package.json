{
  "name": "meteor-paypal-express",
  "version": "1.0.0",
  "description": "A Reaction plugin that allows you to process payments using Paypal Express",
  "main": "register.js",
  "scripts": {
    "lint": "eslint .",
    "lintfix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reaction-contrib/meteor-paypal-express.git"
  },
  "keywords": [
    "reactioncommerce",
    "paypal",
    "ecommerce"
  ],
  "author": "Reaction Community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reaction-contrib/meteor-paypal-express/issues"
  },
  "homepage": "https://github.com/reaction-contrib/meteor-paypal-express#readme",
  "dependencies": {
    "paypal-rest-sdk": "^1.8.1"
  },
  "devDependencies": {
    "@reactioncommerce/eslint-config": "1.8.0",
    "eslint": "^4.19.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jest": "^21.14.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.8.2"
  },
  "babel": {
    "plugins": [
      [
        "lodash",
        {
          "id": [
            "lodash",
            "recompose"
          ]
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./"
          ],
          "alias": {
            "@reactioncommerce/reaction-collections": "./imports/plugins/core/collections",
            "@reactioncommerce/reaction-components": "./imports/plugins/core/components/lib",
            "@reactioncommerce/reaction-router": "./imports/plugins/core/router/lib",
            "@reactioncommerce/reaction-ui": "./imports/plugins/core/ui/client/components",
            "underscore": "lodash"
          }
        }
      ]
    ],
    "presets": [
      "env",
      "react",
      [
        "@babel/preset-stage-2",
        {
          "decoratorsLegacy": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@reactioncommerce",
    "globals": {
      "Alerts": true,
      "Assets": true,
      "jasmine": true,
      "jest/globals": true,
      "Package": true
    },
    "settings": {
      "import/resolver": "meteor"
    },
    "rules": {
      "valid-jsdoc": [
        1,
        {
          "prefer": {
            "arg": "param",
            "argument": "param"
          }
        }
      ]
    }
  }
}
